    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#geographic -->

    <owl:ObjectProperty rdf:about="&dams;geographic">
        <rdfs:domain rdf:resource="&dams;DAMSResource"/>
        <rdfs:subPropertyOf rdf:resource="&dams;subject"/>
        <rdfs:range rdf:resource="&mads;Geographic"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#hasCollection -->

    <owl:ObjectProperty rdf:about="&dams;hasCollection">
        <rdfs:comment>Links from a parent Collection to a child Collection.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;Collection"/>
        <rdfs:domain rdf:resource="&dams;Collection"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#hasComponent -->

    <owl:ObjectProperty rdf:about="&dams;hasComponent">
        <rdfs:comment>Links from a parent Component or Object to a Component it contains.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;Component"/>
