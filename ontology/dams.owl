            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dams;licenseURI"/>
                <owl:allValuesFrom rdf:resource="&xsd;anyURI"/>
