        <rdfs:domain rdf:resource="&dams;SourceCapture"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#scannerModelName -->

    <owl:DatatypeProperty rdf:about="&dams;scannerModelName">
