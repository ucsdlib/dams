        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#scale -->

    <owl:DatatypeProperty rdf:about="&dams;scale">
        <rdfs:comment>The relative size of the representation or rendering of geographic information
