            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Component"/>
                    <rdf:Description rdf:about="&dams;Object"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#hasFile -->

    <owl:ObjectProperty rdf:about="&dams;hasFile">
