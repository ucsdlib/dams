        <rdfs:domain rdf:resource="&dams;SourceCapture"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#scanningSoftware -->

    <owl:DatatypeProperty rdf:about="&dams;scanningSoftware">
        <rdfs:comment>Name of the software application used to scan an image.</rdfs:comment>
