    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY error "http://org.semanticweb.owlapi/error#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY dams "http://library.ucsd.edu/ontology/dams/#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://library.ucsd.edu/ontology/dams/#"
     xml:base="http://library.ucsd.edu/ontology/dams/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:error="http://org.semanticweb.owlapi/error#"
     xmlns:dams="http://library.ucsd.edu/ontology/dams/#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:mads="http://www.loc.gov/mads/rdf/v1#">
    <owl:Ontology rdf:about="http://library.ucsd.edu/ontology/dams/#">
        <rdfs:label>DAMS Data Model</rdfs:label>
        <owl:versionInfo>0.3</owl:versionInfo>
        <rdfs:comment>OWL ontology for DAMS content.  Developed at The UC San Diego
      Library by Esm√© Cowles, Matt Critchlow, Arwen Hutt, Longshou Situ and Brad
      Westbrook.</rdfs:comment>
        <owl:imports rdf:resource="http://www.loc.gov/mads/rdf/v1#"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://library.ucsd.edu/ontology/dams/#assembledColleciton -->

    <owl:ObjectProperty rdf:about="&dams;assembledColleciton">
        <rdfs:comment>Links from an Object to an AssembledCollection that contains it.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;AssembledCollection"/>
        <rdfs:domain rdf:resource="&dams;Object"/>
        <rdfs:subPropertyOf rdf:resource="&dams;collection"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#builtWorkPlace -->

    <owl:ObjectProperty rdf:about="&dams;builtWorkPlace">
        <rdfs:range rdf:resource="&dams;BuiltWorkPlace"/>
        <rdfs:domain rdf:resource="&dams;DAMSResource"/>
        <rdfs:subPropertyOf rdf:resource="&dams;subject"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#cartographics -->

    <owl:ObjectProperty rdf:about="&dams;cartographics">
        <rdfs:comment>Links a Subject to a Cartographics.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;Cartographics"/>
        <rdfs:domain rdf:resource="&dams;Object"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#collection -->

    <owl:ObjectProperty rdf:about="&dams;collection">
        <rdfs:comment>Links from a child Collection to its parent Collection.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;Collection"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Collection"/>
                    <rdf:Description rdf:about="&dams;Object"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#complexSubject -->

    <owl:ObjectProperty rdf:about="&dams;complexSubject">
        <rdfs:domain rdf:resource="&dams;DAMSResource"/>
        <rdfs:subPropertyOf rdf:resource="&dams;subject"/>
        <rdfs:range rdf:resource="&mads;ComplexSubject"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#component -->

    <owl:ObjectProperty rdf:about="&dams;component">
        <rdfs:comment>Links a Component or File to the Component that contains it.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;Component"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Component"/>
                    <rdf:Description rdf:about="&dams;File"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#conferenceName -->

    <owl:ObjectProperty rdf:about="&dams;conferenceName">
        <rdfs:subPropertyOf rdf:resource="&dams;name"/>
        <rdfs:range rdf:resource="&mads;ConferenceName"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;DAMSResource"/>
                    <rdf:Description rdf:about="&dams;Relationship"/>
                    <rdf:Description rdf:about="&mads;ComplexSubject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#copyright -->

    <owl:ObjectProperty rdf:about="&dams;copyright">
        <rdfs:comment>Links an item to a Copyright statement.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;Copyright"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Component"/>
                    <rdf:Description rdf:about="&dams;Object"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#corporatename -->

    <owl:ObjectProperty rdf:about="&dams;corporatename">
        <rdfs:subPropertyOf rdf:resource="&dams;name"/>
        <rdfs:range rdf:resource="&mads;CorporateName"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;DAMSResource"/>
                    <rdf:Description rdf:about="&dams;Relationship"/>
                    <rdf:Description rdf:about="&mads;ComplexSubject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#culturalContext -->

    <owl:ObjectProperty rdf:about="&dams;culturalContext">
        <rdfs:range rdf:resource="&dams;CulturalContext"/>
        <rdfs:domain rdf:resource="&dams;DAMSResource"/>
        <rdfs:subPropertyOf rdf:resource="&dams;subject"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#custodialResponsibilityNote -->

    <owl:ObjectProperty rdf:about="&dams;custodialResponsibilityNote">
        <rdfs:range rdf:resource="&dams;CustodialResponsibilityNote"/>
        <rdfs:domain rdf:resource="&dams;Object"/>
        <rdfs:subPropertyOf rdf:resource="&dams;note"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#date -->

    <owl:ObjectProperty rdf:about="&dams;date">
        <rdfs:comment>Links an item to a Date.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;Date"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Copyright"/>
                    <rdf:Description rdf:about="&dams;DAMSResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#event -->

    <owl:ObjectProperty rdf:about="&dams;event">
        <rdfs:comment>Links a metadata record to an Event that is related to it.</rdfs:comment>
        <rdfs:domain rdf:resource="&dams;DAMSThing"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;DAMSEvent"/>
                    <rdf:Description rdf:about="&mads;MADSType"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#familyName -->

    <owl:ObjectProperty rdf:about="&dams;familyName">
        <rdfs:subPropertyOf rdf:resource="&dams;name"/>
        <rdfs:range rdf:resource="&mads;FamilyName"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;DAMSResource"/>
