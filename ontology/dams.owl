        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#scannerManufacturer -->

    <owl:DatatypeProperty rdf:about="&dams;scannerManufacturer">
