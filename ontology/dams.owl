        <rdfs:domain rdf:resource="&dams;Object"/>
        <rdfs:range rdf:resource="&dams;ProvenanceCollectionPart"/>
        <rdfs:subPropertyOf rdf:resource="&dams;collection"/>
    </owl:ObjectProperty>
    


