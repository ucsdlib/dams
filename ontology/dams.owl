    <!-- http://library.ucsd.edu/ontology/dams/#provenanceCollection -->

    <owl:ObjectProperty rdf:about="&dams;provenanceCollection">
        <rdfs:comment>Links from a child ProvenanceCollectionPart to a parent ProvenanceCollection, or from an Object to a ProvenanceCollection that contains it.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;ProvenanceCollection"/>
        <rdfs:subPropertyOf rdf:resource="&dams;collection"/>
