        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Object"/>
                    <rdf:Description rdf:about="&dams;ProvenanceCollectionPart"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#provenanceCollectionPart -->

    <owl:ObjectProperty rdf:about="&dams;provenanceCollectionPart">
