        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#license -->

    <owl:ObjectProperty rdf:about="&dams;license">
        <rdfs:comment>Links an item to a License.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;License"/>
        <rdfs:domain>
