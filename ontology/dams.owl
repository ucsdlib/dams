                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#name -->

    <owl:ObjectProperty rdf:about="&dams;name">
        <rdfs:comment>Links a Relationship to a Name.</rdfs:comment>
        <rdfs:range rdf:resource="&mads;Name"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;DAMSResource"/>
