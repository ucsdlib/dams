    <owl:DatatypeProperty rdf:about="&dams;preservationLevel">
        <rdfs:comment>Level of preservation of the file (bit-level, full, 0, 1, 2, fully supported
      with future migrations).  Neccessary?</rdfs:comment>
        <rdfs:domain rdf:resource="&dams;File"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">0</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">1</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">2</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first rdf:datatype="&xsd;string">bit-level</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first rdf:datatype="&xsd;string">full</rdf:first>
                                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#projection -->

    <owl:DatatypeProperty rdf:about="&dams;projection">
        <rdfs:comment>See http://en.wikipedia.org/wiki/List_of_map_projections</rdfs:comment>
        <rdfs:comment>The method of representing the surface of a sphere or other three-dimensional
      body on a plane.</rdfs:comment>
        <rdfs:domain rdf:resource="&dams;Cartographics"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#quality -->

    <owl:DatatypeProperty rdf:about="&dams;quality">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Format guidelines:
Images: longest dimension in pixels, e.g. &#39;100px&#39;
Audio: bitrate, e.g. &#39;128kbps&#39;
Video: longest dimension and/or bitrate e.g. &#39;320px&#39;, &#39;640px/1000kbps&#39;</rdfs:comment>
        <rdfs:comment>Format-specific indicator of quality, used to differentiate files with the same use value which differ only by resolution, bitrate, etc.</rdfs:comment>
        <rdfs:domain rdf:resource="&dams;File"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#referenceSystem -->

    <owl:DatatypeProperty rdf:about="&dams;referenceSystem">
        <rdfs:comment>The standard coordinate frame for the coordinates being used (WGS66, WGS72,
      WGS84).</rdfs:comment>
