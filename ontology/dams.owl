        <rdfs:range rdf:resource="&dams;Object"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Component"/>
                    <rdf:Description rdf:about="&dams;File"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#occupation -->

    <owl:ObjectProperty rdf:about="&dams;occupation">
        <rdfs:domain rdf:resource="&dams;DAMSResource"/>
        <rdfs:subPropertyOf rdf:resource="&dams;subject"/>
        <rdfs:range rdf:resource="&mads;Occupation"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#otherRights -->

    <owl:ObjectProperty rdf:about="&dams;otherRights">
        <rdfs:comment>Links an item to an OtherRights.</rdfs:comment>
        <rdfs:range rdf:resource="&dams;OtherRights"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&dams;Component"/>
