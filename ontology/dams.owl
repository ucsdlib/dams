    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#hasProvenanceCollection -->

    <owl:ObjectProperty rdf:about="&dams;hasProvenanceCollection">
        <rdfs:comment>Links from a parent Collection to a child ProvenanceCollection</rdfs:comment>
        <rdfs:domain rdf:resource="&dams;Collection"/>
        <rdfs:range rdf:resource="&dams;ProvenanceCollection"/>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#iconography -->

    <owl:ObjectProperty rdf:about="&dams;iconography">
        <rdfs:domain rdf:resource="&dams;DAMSResource"/>
        <rdfs:range rdf:resource="&dams;Iconography"/>
