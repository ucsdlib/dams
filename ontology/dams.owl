                    <rdf:Description rdf:about="&mads;ComplexSubject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://library.ucsd.edu/ontology/dams/#function -->

    <owl:ObjectProperty rdf:about="&dams;function">
        <rdfs:domain rdf:resource="&dams;DAMSResource"/>
        <rdfs:range rdf:resource="&dams;Function"/>
